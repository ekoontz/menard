{
 "el"
 [{:agr {:gender :masc
         :number :sing}
   :sem {:pred :the}}   

  ;; this is for exceptional feminine nouns
  ;; e.g. "el acta","el agua":
  {:agr {:gender :fem
         :number :sing}
   :is-el-noun? true
   :sem {:pred :the}}]

 "la"
  [{:agr {:gender :fem
          :number :sing}
    :sem {:pred :the}}]

 "las"
 [{:agr {:gender :fem
         :number :plur}
   :sem {:pred :the}}]
   
 "los"
 [{:agr {:gender :masc
         :number :plur}
   :sem {:pred :the}}]

 "un"
 [{:agr {:gender :masc
         :number :sing}
   :sem {:pred :some}}]
 
 "una"
 [{:agr {:gender :fem
         :number :sing}
   :sem {:pred :some}}]
 
 }
