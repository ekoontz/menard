      :derivation {:rules-matched {:runtime-noun-analysis {:morphology-noun-plur-2 true}}}
      :pronoun false
      :agr {:number :plur}
      :regular true}}
 
 {:g [#"^(.*[cs]h)$"  "$1es"] ;;   ash   -> ashes
