[{:g [#"^(.*)$"       "$1"] ;; no change in inflection for singular form.
  :p [#"^(.*)$"       "$1"] ;; no change in inflection for singular form.
  :u {:cat :noun
      :derivation {:rules-matched {:runtime-noun-analysis {:morphology-noun-sing true}}}
      :pronoun false
      :regular true
      :agr {:number :sing}}}

 {:g [#"^(.*[^aeiou])[y]$"  "$1ies"] ;; puppy     -> puppies
  :p [#"^(.*)ies$"     "$1y"] 
  :u {:cat :noun
      :pronoun false
      :derivation {:rules-matched {:runtime-noun-analysis {:morphology-noun-plur-1 true}}}
      :agr {:number :plur}
      :regular true}}
      
 {:g [#"^(.*)fe$"   "$1ves"] ;; life -> lives
